{
	"schemaVersion": 1,
	"id": "ir",
	"version": "${version}",
	"name": "InventoryReader",
	"description": "Skyblock Mining Inventory Reader! Reads a count of items in your inventory! Access at http://localhost:5000",
	"authors": [
		"Scholiboi"
	],
	"contact": {
		"homepage": "https://github.com/Scholiboi/hypixel-forge",
		"sources": "https://github.com/Scholiboi/hypixel-forge",
		"issues": "https://github.com/Scholiboi/hypixel-forge/issues"
	},
	"license": "MIT",
	"icon": "assets/ir/icon.png",
	"environment": "client",
	"entrypoints": {
  		"main": [
    		"inventoryreader.ir.InventoryReader"
  		],
  		"client": [
			"inventoryreader.ir.StorageViewerMod",
			"inventoryreader.ir.InventoryReaderClient",
			"inventoryreader.ir.IrCommandManager"
  		]
	},
	"mixins": [
		"ir.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.3",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*"
	}
}